{
  "name": "kirby-plausible",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "dev": "kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "kirbyup": "^3.3.0",
    "prettier": "^3.5.3"
  }
}
